            android:enabled="true" >
        </service>

       <receiver android:name="com.mobisage.android.MobiSageAdReceiver">
              <intent-filter>
               <action android:name="android.intent.action.PACKAGE_ADDED"/>
                <data android:scheme="package"></data>
            </intent-filter>
         </receiver>

